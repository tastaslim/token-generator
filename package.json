{
  "name": "token-generator-taslim",
  "version": "2.0.0",
  "description": "Authentication and Authorization Handler for Taslim's API Services",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc && cp ./package.json ./dist/",
    "start": "node ./dist/index.js",
    "build:google": "rm -rf ./dist && tsc",
    "build:graph": "rm -rf ./dist && tsc",
    "doc": "jsdoc -c jsdoc.json",
    "release": "npm run build && npm publish ./dist",
    "lint": "eslint . --ext .ts --fix",
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "format": "prettier --config .prettierrc . --write"
  },
  "author": "Team Taslim",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/base64-url": "^2.2.0",
    "@types/base64url": "^2.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.5",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "base64url": "^3.0.1",
    "crypto": "^1.0.1",
    "fs-extra": "^11.1.1",
    "jsdoc": "^4.0.2",
    "jsonwebtoken": "^9.0.1",
    "qs": "^6.11.2",
    "redis": "^4.6.7",
    "rm": "^0.1.8"
  }
}
